{
  "name": "any-balance-devtools",
  "version": "1.0.0",
  "main": "./build/app/app.js",
  "bin": "./build/app/app.js",
  "license": "MIT",
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.11",
    "@types/got": "^9.6.11",
    "@types/http-errors": "^1.8.0",
    "@types/puppeteer": "^3.0.2",
    "@types/shelljs": "^0.8.8",
    "@types/uuid": "^8.3.0",
    "@types/vhost": "^3.0.2",
    "@types/yargs": "^17.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@types/google-closure-compiler": "^0.0.18",
    "app-root-path": "^3.0.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "google-closure-compiler": "^20210601.0.0",
    "got": "^11.8.0",
    "hpagent": "^0.1.1",
    "http-errors": "^1.8.0",
    "log4js": "^6.3.0",
    "puppeteer": "^5.4.0",
    "shelljs": "^0.8.4",
    "socks-proxy-agent": "^5.0.0",
    "uuid": "^8.3.2",
    "xmldom-ts": "^0.3.1",
    "xpath-ts": "^1.3.13",
    "yargs": "^17.0.1"
  },
  "scripts": {
    "tsc": "tsc",
    "build": "tsc && pkg .",
    "postbuild": "copy -R ./node_modules/puppeteer/.local-chromium dist/chromium",
    "dev": "ts-node-dev --respawn --transpile-only ./app/app.ts",
    "devt": "ts-node-dev --respawn --transpile-only ./app/test.ts",
    "debug": "tsc && node --inspect-brk ./build/app/app.js",
    "debugt": "tsc && node --inspect-brk ./build/app/test.js"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": "res/**/*",
    "targets": [
      "node12-win-x64"
    ],
    "outputPath": "dist"
  }
}
